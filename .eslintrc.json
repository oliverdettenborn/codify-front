{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": ["off"],
        "no-alert": "off",
        "no-return-assign": "off",
        "no-tabs": "off",
        "react/jsx-props-no-spreading": ["off"]
    }
}
